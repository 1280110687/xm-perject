{
    "singleQuote": false,
    "trailingComma": "all",
    "semi": true,
    "bracketSpacing": true,
    "endOfLine": "auto",
    "tabWidth": 2,
    "htmlWhitespaceSensitivity": "ignore",
    "arrowParens": "avoid",
    "prettier/prettier": [
        "error",
        {
            "singleQuote": true,
            "usePrettierrc": true,
            "parser": "flow"
        }
    ]
}